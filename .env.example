# Server configuration
PORT=3000

# MongoDB connection
MONGODB_URI=mongodb://localhost/coffee-shop

# OneSignal configuration
ONESIGNAL_APP_ID=your-onesignal-app-id
ONESIGNAL_REST_API_KEY=your-onesignal-rest-api-key

# JWT configuration
